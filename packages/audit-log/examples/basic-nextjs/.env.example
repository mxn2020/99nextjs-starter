# Environment Configuration for Basic Next.js Audit Log Example

# Audit Log Configuration
AUDIT_LOG_DIRECTORY=./audit-logs
AUDIT_LOG_LEVEL=medium
AUDIT_BATCH_SIZE=10
AUDIT_FLUSH_INTERVAL=5000
AUDIT_MAX_FILE_SIZE=10485760
AUDIT_MAX_FILES=5

# Application Configuration
NEXT_PUBLIC_APP_NAME=Basic Audit Example
NEXT_PUBLIC_APP_VERSION=1.0.0

# Development Settings
NODE_ENV=development
NEXT_PUBLIC_AUDIT_DEBUG=true
